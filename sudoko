import random
play = ""
repeat = True
rotate = 0
rotation = 1000

def main_table(sudoku_table):
    row = 0
    column = 0
    a = 0
    print(">    " ,">" ," Suduko Table  <")
    print("----------------------------------")
    while a <= 8:
        print("   " ,*sudoku_table[ row + 0: 3+ column], " | ", *sudoku_table[row + 3: 6 + column], " | ", *sudoku_table[row + 6: 9 + column])
        if a == 2:
            print("----------------------------------")
        if a == 5:
            print("----------------------------------")
        column += 9
        row += 9
        a += 1
    print("----------------------------------")


def row_four(sudoku_table):
    num_pick = [x for x in range(1, 10)]
    variables = [], [], [], []
    last_value, box_4_3, box_4_2, box_4_1 = variables

    if a in [27]:
        for x in [0, 9, 18]:
            box_4_1.append(sudoku_table[x])

        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [28]:
        for x in [1, 10, 19]:
            box_4_1.append(sudoku_table[x])
        num_pick = [x for x in num_pick if x not in h_r1_c4]
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [29]:
        for x in [2, 11, 20]:
            box_4_1.append(sudoku_table[x])
        num_pick = [x for x in num_pick if x not in h_r1_c4]
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [30]:
        for x in [3, 12, 21]:
            box_4_1.append(sudoku_table[x])
        num_pick = [x for x in num_pick if x not in h_r1_c4]
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [31]:
        for x in [4, 13, 22]:
            box_4_1.append(sudoku_table[x])
        num_pick = [x for x in num_pick if x not in h_r1_c4]
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [32]:
        for x in [5, 14, 23]:
            box_4_1.append(sudoku_table[x])
            ver_5.append(sudoku_table[x])
        num_pick = [x for x in num_pick if x not in h_r1_c4]
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c4.append(sudoku_table[a])

    elif a in [33]:
        for x in [6, 15, 24]:
            box_4_1.append(sudoku_table[x])
            ver_6.append(sudoku_table[x])
        last_value = [x for x in num_pick if x not in h_r1_c4]
        random.shuffle(last_value)
        if last_value[0] not in box_4_1:
            sudoku_table[a] = last_value[0]
            h_r1_c4.append(sudoku_table[a])

        elif last_value[1] not in box_4_1:
            sudoku_table[a] = last_value[1]
            h_r1_c4.append(sudoku_table[a])

        elif last_value[2] not in box_4_1:
            sudoku_table[a] = last_value[2]
            h_r1_c4.append(sudoku_table[a])

        elif last_value[0] in box_4_1:
            if last_value[0] not in ver_5 and h_r1_c4[-1] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-1]
                sudoku_table[32] = last_value[0]
                h_r1_c4.append(h_r1_c4[-1])
                h_r1_c4[5] = last_value[0]
            else:
                pass

    elif a in [34]:
        for x in [7, 16, 25]:
            box_4_1.append(sudoku_table[x])
            ver_7.append(sudoku_table[x])
        last_value = [x for x in num_pick if x not in h_r1_c4]

        random.shuffle(last_value)
        if last_value[0] not in box_4_1:
            sudoku_table[a] = last_value[0]
            h_r1_c4.append(sudoku_table[a])
        elif last_value[1] not in box_4_1:
            sudoku_table[a] = last_value[1]
            h_r1_c4.append(sudoku_table[a])

        elif last_value[0] in box_4_1:
            if last_value[0] not in ver_6 and h_r1_c4[-1] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-1]
                sudoku_table[33] = last_value[0]
                h_r1_c4.append(h_r1_c4[-1])
                h_r1_c4[6] = last_value[0]

            elif last_value[0] not in ver_5 and h_r1_c4[-2] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-2]
                sudoku_table[32] = last_value[0]
                h_r1_c4.append(h_r1_c4[-2])
                h_r1_c4[5] = last_value[0]

            elif last_value[0] not in ver_4 and h_r1_c4[-3] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-3]
                sudoku_table[31] = last_value[0]
                h_r1_c4.append(h_r1_c4[-3])
                h_r1_c4[4] = last_value[0]

            else:
                pass

    elif a in [35]:
        for x in [8, 17, 26]:
            box_4_1.append(sudoku_table[x])
        last_value = [x for x in num_pick if x not in h_r1_c4]
        random.shuffle(last_value)
        if last_value[0] not in box_4_1:
            sudoku_table[a] = last_value[0]
            h_r1_c4.append(sudoku_table[a])
        elif last_value[0] in box_4_1:
            if last_value[0] not in ver_7 and h_r1_c4[-1] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-1]
                sudoku_table[34] = last_value[0]
                h_r1_c4.append(h_r1_c4[-1])
                h_r1_c4[7] = last_value[0]

            elif last_value[0] not in ver_6 and h_r1_c4[-2] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-2]
                sudoku_table[33] = last_value[0]
                h_r1_c4.append(h_r1_c4[-2])
                h_r1_c4[6] = last_value[0]

            elif last_value[0] not in ver_5 and h_r1_c4[-3] not in box_4_1:
                sudoku_table[a] = h_r1_c4[-3]
                sudoku_table[32] = last_value[0]
                h_r1_c4.append(h_r1_c4[-3])
                h_r1_c4[5] = last_value[0]
            else:
                pass


def row_five(sudoku_table):
    num_pick = [x for x in range(1, 10)]
    variables = [], [], [], []
    last_value, box_4_3, box_4_2, box_4_1 = variables
    variables1 = [], [], [], [], [], [], [], [], []
    hor_ver0, hor_ver1, hor_ver2, hor_ver3, hor_ver4, hor_ver5, hor_ver6, hor_ver7, hor_ver8 = variables1

    if a in [36]:
        for x in [0, 9, 18, 27]:
            box_4_1.append(sudoku_table[x])
            ver_0.append(sudoku_table[x])
        for x in [27, 28, 29]:
            box_4_1.append(sudoku_table[x])
        box_4_1 = list(dict.fromkeys(box_4_1))
        last_value = [x for x in num_pick if x not in box_4_1]
        random.shuffle(last_value)
        sudoku_table[a] = last_value[0]
        h_r1_c5.append(sudoku_table[a])

    if a in [37]:
        for x in [1, 10, 19, 28]:
            box_4_1.append(sudoku_table[x])
            ver_1.append(sudoku_table[x])
        for x in [27, 28, 29, 36]:
            box_4_1.append(sudoku_table[x])
