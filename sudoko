            sudoku_table[a] = last_value[4]
            h_r1_c5.append(sudoku_table[a])
        else:
            pass

    if a in [41]:
        ver_temp_1 = []
        ver_temp_2 = []
        for x in [5, 14, 23, 32]:
            box_4_1.append(sudoku_table[x])
            ver_5.append(sudoku_table[x])
        for x in [30, 31, 32, 39, 40]:
            box_4_1.append(sudoku_table[x])
        box_4_1 = list(dict.fromkeys(box_4_1))
        for x in [5, 14, 23, 32]:
            ver_temp_1.append(sudoku_table[x])
            ver_temp_2.append(sudoku_table[x])
        for x in [30, 31, 32, 39]:
            ver_temp_1.append(sudoku_table[x])
        for x in [30, 31, 32, 40]:
            ver_temp_2.append(sudoku_table[x])
        ver_temp_2 = list(dict.fromkeys(ver_temp_2))
        ver_temp_1 = list(dict.fromkeys(ver_temp_1))
