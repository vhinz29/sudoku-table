            sudoku_table[a] = last_value[2]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[3] not in box_4_1:
            sudoku_table[a] = last_value[3]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[4] not in box_4_1:
            sudoku_table[a] = last_value[4]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[5] not in box_4_1:
            sudoku_table[a] = last_value[5]
            h_r1_c5.append(sudoku_table[a])
        else:
            pass

    if a in [40]:
        for x in [4, 13, 22, 31]:
            box_4_1.append(sudoku_table[x])
            ver_4.append(sudoku_table[x])
        for x in [30, 31, 32, 39]:
