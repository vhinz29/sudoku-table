            hor_ver2.append(x)
            hor_ver1.append(x)
            hor_ver0.append(x)
        for x in box_row5:
            hor_ver3.append(x)
            hor_ver4.append(x)
            hor_ver5.append(x)
        hor_ver0 = list(dict.fromkeys(hor_ver0))
        hor_ver1 = list(dict.fromkeys(hor_ver1))
        hor_ver2 = list(dict.fromkeys(hor_ver2))
        hor_ver3 = list(dict.fromkeys(hor_ver3))
        hor_ver4 = list(dict.fromkeys(hor_ver4))
        hor_ver5 = list(dict.fromkeys(hor_ver5))
        box_4_1 = list(dict.fromkeys(box_4_1))
        last_value = [x for x in num_pick if x not in h_r1_c5]
        random.shuffle(last_value)
        if last_value[0] not in box_4_1:
            sudoku_table[a] = last_value[0]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[1] not in box_4_1:
