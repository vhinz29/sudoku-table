        elif last_value[2] not in box_4_1:
            sudoku_table[a] = last_value[2]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[3] not in box_4_1:
            sudoku_table[a] = last_value[3]
            h_r1_c5.append(sudoku_table[a])
        elif last_value[0] in box_4_1:
            if last_value[0] not in hor_ver4 and h_r1_c5[-1] not in ver_temp_1:
                sudoku_table[a] = h_r1_c5[-1]
                sudoku_table[40] = last_value[0]
                h_r1_c5.append(sudoku_table[a])
                h_r1_c5[4] = last_value[0]
            elif last_value[0] not in hor_ver3 and h_r1_c5[-2] not in ver_temp_2:
                sudoku_table[a] = h_r1_c5[-2]
                sudoku_table[39] = last_value[0]
                h_r1_c5.append(sudoku_table[a])
                h_r1_c5[3] = last_value[0]
            elif last_value[0] not in hor_ver2 and h_r1_c5[-3] not in box_4_1:
                sudoku_table[a] = h_r1_c5[-3]
